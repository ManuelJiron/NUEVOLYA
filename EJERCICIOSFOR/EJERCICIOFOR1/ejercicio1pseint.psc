Proceso ejercicio_1
	// Dado un número natural n se desea calcular la suma de los números naturales desde 1 hasta n.
	Definir numeroNatural Como Entero;
	Definir resultado Como Entero;
	Escribir "Dame un numero";
	Leer numeroNatural;
	resultado <- numeroNatural*(numeroNatural+1)/2;
	Escribir "El resultado de la suma de 1 hasta ", numeroNatural, " es: ", resultado;
FinProceso
